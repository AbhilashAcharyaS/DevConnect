{
    "installCommand":"npm install --legacy-peer-deps",
    "version":2,
    "builds":[
        {
            "src":"app.js",
            "use":"@vercel/node",
            "config":{ "includeFiles":["dist/**"] }
        },
        {
            "src":"src/**/*",
            "use":"@vercel/static"
        }
    ],
    "routes":[
        {"src":"/(.*)", "dest":"app.js"}
    ]
}